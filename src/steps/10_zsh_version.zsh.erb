# Check Zsh version
autoload -Uz is-at-least && if ! is-at-least <%= min_zsh_version %>; then
  print -u2 -R "${ZERROR}You're using Zsh version ${ZSH_VERSION} and versions < <%= min_zsh_version %> are not supported. Please update your Zsh.${ZNORMAL}"
  return 1
fi
print -R "${ZOKAY}Using Zsh version ${ZSH_VERSION}"
